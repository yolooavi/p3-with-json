      "adoptionForm": {
        "title": "Formulario de adopción de gato",
        "formFields": {
          "personalData": {
            "legend": "Datos Personales",
            "fields": [
              {
                "label": "Nombre y Apellido",
                "type": "text",
                "id": "name",
                "name": "name",
                "class": "form-control w-100",
                "required": true
              },
              {
                "label": "Teléfono",
                "type": "tel",
                "id": "phone",
                "name": "phone",
                "class": "form-control w-100",
                "required": true
              },
              {
                "label": "Correo Electrónico",
                "type": "email",
                "id": "email",
                "name": "email",
                "class": "form-control w-100",
                "required": true
              }
            ]
          },
          "catSelection": {
            "legend": "Selección de Gato",
            "fields": [
              {
                "label": "Selecciona un gato",
                "type": "select",
                "id": "cat",
                "name": "cat",
                "options": ["Gatito", "Lena", "Felix", "Luna", "Simba"],
                "required": true
              },
              {
                "label": "Motivo de la adopción",
                "type": "textarea",
                "id": "reason",
                "name": "reason",
                "rows": 4,
                "cols": 30,
                "required": true
              }
            ]
          },
          "adoptionConditions": {
            "legend": "Condiciones de Adopción",
            "fields": [
              {
                "label": "Tipo de vivienda",
                "type": "radio",
                "name": "home_type",
                "options": [
                  {
                    "id": "home_apartment",
                    "value": "apartment",
                    "label": "Apartamento"
                  },
                  {
                    "id": "home_house",
                    "value": "house",
                    "label": "Casa"
                  }
                ],
                "required": true
              },
              {
                "label": "¿Tienes otras mascotas?",
                "type": "checkbox",
                "id": "other_pets",
                "name": "other_pets"
              },
              {
                "label": "¿Has tenido un gato antes?",
                "type": "checkbox",
                "id": "experience",
                "name": "experience"
              }
            ]
          }
        },
        "buttons": [
